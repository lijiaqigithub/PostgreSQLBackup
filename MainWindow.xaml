<Window x:Class="PostgreSQLBackup.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PostgreSQLBackup"
        mc:Ignorable="d"
        Loaded="onLoad"
        Title="Backup Postgre Database" Height="200" Width="400">
    <Grid>
        <Button Grid.Column="0" Grid.Row="1" x:Name="backupButton" Content="Creare fisier backup" Height="50" Width="150" Click="backupButton_Click"></Button>
    </Grid>
</Window>
